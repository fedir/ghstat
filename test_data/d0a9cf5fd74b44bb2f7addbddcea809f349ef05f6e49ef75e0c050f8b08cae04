rings(names)\r\n\r\n\tfor _, n := range names {\r\n\t\tmatched, err := Match(pattern, n)\r\n\t\tif err != nil {\r\n\t\t\treturn m, err\r\n\t\t}\r\n\t\tif matched {\r\n\t\t\tm = append(m, Join(dir, n))\r\n\t\t}\r\n\t}\r\n\treturn\r\n}\r\n\r\n","score":1.0},{"url":"https://api.github.com/repos/golang/go/issues/25307","repository_url":"https://api.github.com/repos/golang/go","labels_url":"https://api.github.com/repos/golang/go/issues/25307/labels{/name}","comments_url":"https://api.github.com/repos/golang/go/issues/25307/comments","events_url":"https://api.github.com/repos/golang/go/issues/25307/events","html_url":"https://github.com/golang/go/issues/25307","id":321549380,"number":25307,"title":"spec: EBNF incorrectly states Receiver can be multiple parameters","user":{"login":"iivvoo","id":540122,"avatar_url":"https://avatars0.githubusercontent.com/u/540122?v=4","gravatar_id":"","url":"https://api.github.com/users/iivvoo","html_url":"https://github.com/iivvoo","followers_url":"https://api.github.com/users/iivvoo/followers","following_url":"https://api.github.com/users/iivvoo/following{/other_user}","gists_url":"https://api.github.com/users/iivvoo/gists{/gist_id}","starred_url":"https://api.github.com/users/iivvoo/starred{/owner}{/repo}","subscriptions_url":"https://api.github.com/users/iivvoo/subscriptions","organizations_url":"https://api.github.com/users/iivvoo/orgs","repos_url":"https://api.github.com/users/iivvoo/repos","events_url":"https://api.github.com/users/iivvoo/events{/privacy}","received_events_url":"https://api.github.com/users/iivvoo/received_events","type":"User","site_admin":false},"labels":[{"id":373402289,"url":"https://api.github.com/repos/golang/go/labels/NeedsInvestigation","name":"NeedsInvestigation","color":"ededed","default":false}],"state":"closed","locked":false,"assignee":null,"assignees":[],"milestone":{"url":"https://api.github.com/repos/golang/go/milestones/62","html_url":"https://github.com/golang/go/milestone/62","labels_url":"https://