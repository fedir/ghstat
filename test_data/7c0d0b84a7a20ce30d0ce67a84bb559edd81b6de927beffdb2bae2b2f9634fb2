\nfrom unittest import TestCase\r\n\r\nimport requests\r\nfrom bottle import Bottle, run\r\n\r\n# https://github.com/bottlepy/bottle/blob/0dd195111621b4db5e9b7d5e5852fab3c6270098/bottle.py#L2011\r\napp = Bottle(autojson=True)\r\n\r\n\r\n@app.get('/dict')\r\ndef dict_view():\r\n    return {'result': 1}\r\n\r\n\r\n@app.get('/list')\r\ndef list_view():\r\n    return [\"result\"]\r\n\r\n\r\n@app.get('/tuple')\r\ndef tuple_view():\r\n    return \"result\",\r\n\r\n\r\nt = Thread(target=lambda a: run(app=a), args=(app,))\r\nt.daemon = __name__ != \"__main__\"\r\nt.start()\r\n\r\nurl = 'http://127.0.0.1:8080'\r\n\r\n\r\nclass AutoJSONTestCase(TestCase):\r\n    @classmethod\r\n    def setUpClass(cls):\r\n        # Wait for web-server to start\r\n        sleep(1)\r\n\r\n    def test_dict(self):\r\n        response = requests.get(url + '/dict')\r\n        self.assertEqual(response.status_code, 200)\r\n        self.assertEqual(response.headers.get('Content-Type'), 'application/json')\r\n        self.assertEqual(response.text, '{\"result\": 1}')\r\n        pass\r\n\r\n    def test_list(self):\r\n        response = requests.get(url + '/list')\r\n        self.assertEqual(response.status_code, 200)\r\n        self.assertEqual(response.headers.get('Content-Type'), 'application/json')\r\n        self.assertEqual(response.text, '[\"result\"]')\r\n\r\n    def test_tuple(self):\r\n        response = requests.get(url + '/tuple')\r\n        self.assertEqual(response.status_code, 200)\r\n        self.assertEqual(response.headers.get('Content-Type'), 'application/json')\r\n        self.assertEqual(response.text, '[\"result\"]')\r\n\r\n```\r\n\r\n","performed_via_github_app":null,"score":1.0},{"url":"https://api.github.com/repos/mozilla-services/absearch/issues/30","repository_url":"https://api.github.com/repos/mozilla-services/absearch","labels_url":"https://api.github.com/repos/mozilla-services/absearch/issues/30/labels{/name}","comments_url":"https://api.github.com/repos/mozilla-services/